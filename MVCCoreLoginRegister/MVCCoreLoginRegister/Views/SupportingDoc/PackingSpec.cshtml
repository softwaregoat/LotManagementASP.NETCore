@{
    ViewData["Title"] = "Packing Specification";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Packing Specification</h1>
<br />
<div class="row">
    <div class="col-lg-1">
        <label class="col-form-label">Product</label>
    </div>
    <div class="col-lg-2">
        <input type="text" id="product" class="form-control" value="" />
    </div>
</div>
<br />
<div class="row">
    <iframe src="/pdf/test.pdf#zoom=80" type="application/pdf" style="width: 100%;height: 600px;"></iframe>
</div>

@section Scripts {
    <script>
        $(document).ready(function () {
            let product = getCookie('ProductName');
            $('#product').val(product);
            var timer2 = "0:30";
            timerDown(timer2, "/Home/JobBagOverview");
        });
    </script>
}
